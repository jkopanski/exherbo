[Unit]
Description=VNC Per-connection server

[Service]
EnvironmentFile=/etc/conf.d/xvnc.conf
ExecStart=-/usr/x86_64-pc-linux-gnu/bin/Xvnc -inetd $XVNC_OPTS
StandardInput=socket
StandardError=journal
